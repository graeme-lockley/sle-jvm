type List a =
    Nil
  | Cons a (List a)


type Tuple a b =
    Tuple a b


repeat : Int -> a -> (List a)
repeat n e
    | n == 0 = Nil
    | otherwise = Cons e (repeat (n - 1) e)


singleton =
    repeat 1


tupleList x y =
    Cons x (singleton y)


singleIntList =
    singleton 10


doubleIntList =
    tupleList 1 2


doubleBooleanList =
    tupleList True False


intIntTuple =
    Tuple 1 2
