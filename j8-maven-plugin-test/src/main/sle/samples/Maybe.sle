export
    andThen,
    map,
    withDefault


withDefault : a -> Maybe a -> a
withDefault default Nothing =
    default
withDefault default (Just value) =
    value


map : (a -> b) -> Maybe a -> Maybe b
map f Nothing =
    Nothing
map f (Just value) =
    Just (f value)


andThen : (a -> Maybe b) -> Maybe a -> Maybe b
andThen f Nothing =
    Nothing
andThen f (Just value) =
    f value