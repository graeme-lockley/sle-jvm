-- A collection of list extension functions with import and export
export find

import github:Data.List:1.2.1
import github:Data.Maybe exposing Maybe(..)


find : (a -> Bool) -> List a -> Maybe a
find p Nil = Nothing
find p (Cons x xs) =
    if p x then
        Just x
    else
        find p xs
