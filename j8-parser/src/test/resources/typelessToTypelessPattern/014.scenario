-- A simple case expression over a product Tuple
type Tuple a b =
    Tuple a b

first : (Tuple a b) -> a
first t =
    case t of
        Tuple a b -> a

second : (Tuple a b) -> b
second t =
    case t of
        Tuple a b -> b

mapFirst : (a -> c) -> (Tuple a b) -> (Tuple c b)
mapFirst f t =
    case t of
        Tuple a b -> Tuple (f a) b

mapSecond : (b -> c) -> (Tuple a b) -> (Tuple a c)
mapSecond f t =
    case t of
        Tuple a b -> Tuple a (f b)
