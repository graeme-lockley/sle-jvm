-- Translate case with constant string into lambda
stringToNum v =
    case v of
        "Hello" -> 1
        "World" -> 2
        otherwise -> 0
-- enrichedASTpp
stringToNum : <> Data.String -> Data.Int
stringToNum =
  (LAMBDA v ->
    (CALL
      (LAMBDA $case ->
        (BAR
          (CALL
            (LAMBDA $v0 ->
              (IF
                (CALL
                  (CALL
                    (==)
                    "Hello"
                  )
                  $v0
                )
                1
                FAIL
              )
            )
            $case
          )
          (CALL
            (LAMBDA $v0 ->
              (IF
                (CALL
                  (CALL
                    (==)
                    "World"
                  )
                  $v0
                )
                2
                FAIL
              )
            )
            $case
          )
          (CALL
            (LAMBDA otherwise ->
              0
            )
            $case
          )
          ERROR
        )
      )
      v
    )
  )