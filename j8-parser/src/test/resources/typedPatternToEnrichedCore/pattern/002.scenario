-- Translate case with constant int into lambda
numToString v =
    case v of
        0 -> "0"
        1 -> "1"
        x -> "many"
-- enrichedASTpp
numToString : <> Int -> String
numToString =
  (LAMBDA v ->
    (CALL
      (LAMBDA $case ->
        (BAR
          (CALL
            (LAMBDA $v0 ->
              (IF
                (CALL
                  (CALL
                    (==)
                    0
                  )
                  $v0
                )
                "0"
                FAIL
              )
            )
            $case
          )
          (CALL
            (LAMBDA $v0 ->
              (IF
                (CALL
                  (CALL
                    (==)
                    1
                  )
                  $v0
                )
                "1"
                FAIL
              )
            )
            $case
          )
          (CALL
            (LAMBDA x ->
              "many"
            )
            $case
          )
          ERROR
        )
      )
      v
    )
  )