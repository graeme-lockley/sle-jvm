-- Simple let expression
type Tuple a b =
  Tuple a b


slice : Int -> Int -> String -> String
slice a b c =
  c

dropLeft : Int -> String -> String
dropLeft a b =
  b


parseSingleCharacterSeparator input =
  let
    separator =
      slice 2 3 input

    text : String
    text =
      dropLeft 4 text

  in
    (separator, text)
