-- Type record
typealias Person =
  { name : String
  , dateOfBirth :
    { day : Int
    , month : Int
    , year : Int
    }
  }


extractName : Person -> String
extractName person =
  person.name


value : Person
value =
  { name = "Graeme"
  , dateOfBirth =
    { day = 9
    , month = 4
    , year = 1968
    }
  }


main =
  extractName value
