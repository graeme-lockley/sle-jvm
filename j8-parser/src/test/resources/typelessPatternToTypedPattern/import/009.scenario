-- Validate that the type checker will deal identities of types
import file:./List as L1
import file:./List as L2


isEmpty : L2.List a -> Bool
isEmpty L1.Nil = True
isEmpty xs = False


singleton : a -> L1.List a
singleton x =
    L2.Cons x L1.Nil

result =
    isEmpty (singleton "Hello")
