-- Nested pattern
type List a =
    Nil
  | Cons a (List a)

type Tuple a b =
    Tuple a b


map2 : (a -> b -> c) -> List a -> List b -> List c
map2 f a b =
    case Tuple a b of
        Tuple Nil Nil ->
            Nil

        Tuple (Cons x xs) (Cons y ys) ->
            Cons (f x y) (map2 xs ys)

        any ->
            Nil
